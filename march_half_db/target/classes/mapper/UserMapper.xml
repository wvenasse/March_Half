<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.dao.UserDao">
    <select id="getUserByLoginId" resultType="java.lang.Integer">
    select count(id) from march_half_users where userId = #{userId} and password = #{password}
  </select>
</mapper>